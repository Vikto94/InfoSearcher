package searcher;

import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileReader;
import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;

public class TokenSearcher {
//	private final String[] morphemeInFile;
	
	TokenSearcher (String fileName) throws FileNotFoundException{
		Scanner in = new Scanner(new File(fileName));
		String word = "";
		List<String> wordList = new ArrayList<String>();
		while (in.hasNext()) {
			word = in.next();
			wordList.add(word);
			System.out.print("\nw: " + word);
		}
		morphemeInFile = wordList.toArray();
	}
	
	public boolean isFind (String token){
		return false;
	}
}
